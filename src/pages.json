{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
			"^M(.*)": "@/components/M$1/index.vue",
			"^m-(.*)": "@/components/M$1/index.vue"
		}
	},
	"pages": [
		{
			"path": "pages/splash/index",
			"layout": false,
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"subPackages": [
		{
			"root": "subPages/login",
			"name": "login",
			"pages": [
				{
					"path": "index",
					"layout": false,
					"style": {
						"navigationStyle": "custom"
					}
				}
			]
		},
		{
			"root": "subPages/privacy",
			"name": "privacy",
			"pages": [
				{
					"path": "index",
					"style": {
						"navigationBarTitleText": "个人信息保护政策"
					}
				}
			]
		},
		{
			"root": "subPages/auth",
			"name": "auth",
			"pages": [
				{
					"path": "index",
					"style": {
						"navigationStyle": "custom"
					}
				}
			]
		},
		{
			"root": "subPages/test",
			"name": "test",
			"pages": [
				{
					"path": "index",
					"actions": [
						"login"
					],
					"layout": true,
					"style": {
						"navigationStyle": "custom"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uniapp-best",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}